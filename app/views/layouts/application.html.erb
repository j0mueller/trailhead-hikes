<!DOCTYPE html>
<html>
  <head>
    <title>Trailhead</title>
    <%= csrf_meta_tags %>
    <meta name="viewport" content="width=device-width,initial-scale=1 user-scalable=no">
    <link rel="stylesheet" href="http://fonts.googleapis.com/icon?family=Material+Icons">
    <%= stylesheet_link_tag    'application', media: 'all' %>
    <%= javascript_include_tag 'application' %>
    <link rel="stylesheet" href="https://unpkg.com/react-table@latest/react-table.css">
  </head>

  <body>
    <%= render "layouts/navbar" %>

    <% if flash[:notice] %>
      <script type="text/javascript">
      Materialize.toast('<%= notice %>', 5000)
      </script>
    <% elsif flash[:error] %>
      <script type="text/javascript">
      Materialize.toast('<%= flash[:error] %>', 5000)
      </script>
    <% elsif flash[:alert] %>
      <script type="text/javascript">
      Materialize.toast('<%= alert %>', 5000)
      </script>
    <% end %>

    <%= yield %>
  
  <script  src="https://maps.googleapis.com/maps/api/js?key=AIzaSyDCTSBW3qWtSCO8JDTj4cqd3rUKNcPms4Q&libraries=places&callback=initAutocomplete" async defer>
  </script>
  </body>
</html>
